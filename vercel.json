{
    
    "version": 2,
    "builds": [
        {
            "src": "ecommerce/wsgi.py",
            "user": "@vercel/python",
            "config": {"maxLambdaSiize": "15mb", "runtime": "python3.10"}
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/wsgi.py",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ecommerce/wsgi.py"
        }
    ],

    "outputDirectory": "staticfiles_build"

}